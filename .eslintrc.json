{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "plugins": [
        "react"
    ],
    "ecmaFeatures": {
        "modules": true,
        "jsx": true,
        "generators": true
    },
    "rules": {
        "semi": 2,
        "no-console": 1,
        "no-unused-vars": 1
    },
    "globals": {
        "$": true
    }
}
